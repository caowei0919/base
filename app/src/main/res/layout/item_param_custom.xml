<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        android:id="@+id/tv_customName"
        android:layout_width="match_parent"
        android:layout_height="@dimen/H60"
        android:background="@color/c_f8f8fa"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/W25"
        android:text="@string/custom_parameter"
        android:textSize="14sp"
        android:textColor="@color/c_808080">
    </TextView>

    <LinearLayout
        android:id="@+id/lin_customType"
        android:layout_below="@id/lin_dimensionParam"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
    </LinearLayout>
</LinearLayout>